<!DOCTYPE html>
<html>
  <head>
    <%- include('partials/commonhead.ejs') %>
  </head>
  <body>
    <div class="container">
      <form class="d-inline" method="post" action="/">
        <div class="md-form md-outline ">
          <input type="text" id="form1" class="form-control" name="ytlink">
          <label for="form1">Add youtube link</label>
        </div>


        <div class="btn-group btn-group-toggle" data-toggle="buttons">
          <label class="btn btn-secondary">
            <input type="radio" name="options" id="option1" autocomplete="off" value="mp4"> Mp4
          </label>
          <label class="btn btn-secondary">
            <input type="radio" name="options" id="option2" autocomplete="off" value="mp3"> Mp3
          </label>
        </div>

        <button class="btn btn-primary">Convert</button>
      </form>
    </div>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <%- include('partials/commonscript.ejs') %>
  </body>
</html>
